var addLang = document.getElementById("add");
var removeLang = document.getElementById("remove");
var addLang2 = document.getElementById("add2");
var removeLang2 = document.getElementById("remove2");
var fieldset2 = document.getElementById("otherskills");
var progLangId = 0;
function addProgLanguage() {
                progLangId++;
                var divToAdd = document.getElementById('proglang');
                divToAdd.setAttribute('id', 'proglang' + progLangId);
                var parent = document.getElementById('computerskills');
                var childBefore = document.getElementById('remProgLang');
                parent.insertBefore(divToAdd, childBefore);
}